write-kubeconfig-mode: "0644"
token: {{ hostvars[groups['server_nodes'][0]]['token'] }}
server: https://{{ hostvars[groups['server_nodes'][0]]['ansible_host'] }}:9345
tls-san:
  - {{ vip }}
  - {{ hostvars[groups['server_nodes'][0]]['ansible_host'] }}
  - {{ hostvars[groups['server_nodes'][1]]['ansible_host'] }}
  - {{ hostvars[groups['server_nodes'][2]]['ansible_host'] }}
node-label:
  - server=true
